{
  "name": "@rapidraptor/auth-server",
  "version": "1.0.2",
  "type": "module",
  "description": "Server-side authentication library for Node.js/Express applications",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/HOBT0R/rapidraptor.auth.git"
  },
